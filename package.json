{
  "name": "ocd-js",
  "private": false,
  "version": "1.1.10-beta",
  "description": "Opinionated TypeScript backend framework with decorator-first APIs, dependency injection, and batteries-included tooling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ocd": "./packages/cli/dist/index.js"
  },
  "files": [
    "dist",
    "packages",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./packages/core/dist/index.js",
      "require": "./packages/core/dist/index.js",
      "types": "./packages/core/dist/index.d.ts"
    },
    "./server": {
      "import": "./packages/server/dist/index.js",
      "require": "./packages/server/dist/index.js",
      "types": "./packages/server/dist/index.d.ts"
    },
    "./auth": {
      "import": "./packages/auth/dist/index.js",
      "require": "./packages/auth/dist/index.js",
      "types": "./packages/auth/dist/index.d.ts"
    },
    "./security": {
      "import": "./packages/security/dist/index.js",
      "require": "./packages/security/dist/index.js",
      "types": "./packages/security/dist/index.d.ts"
    },
    "./observability": {
      "import": "./packages/observability/dist/index.js",
      "require": "./packages/observability/dist/index.js",
      "types": "./packages/observability/dist/index.d.ts"
    },
    "./performance": {
      "import": "./packages/performance/dist/index.js",
      "require": "./packages/performance/dist/index.js",
      "types": "./packages/performance/dist/index.d.ts"
    },
    "./orm": {
      "import": "./packages/orm/dist/index.js",
      "require": "./packages/orm/dist/index.js",
      "types": "./packages/orm/dist/index.d.ts"
    },
    "./testing": {
      "import": "./packages/testing/dist/index.js",
      "require": "./packages/testing/dist/index.js",
      "types": "./packages/testing/dist/index.d.ts"
    },
    "./plugins": {
      "import": "./packages/plugins/dist/index.js",
      "require": "./packages/plugins/dist/index.js",
      "types": "./packages/plugins/dist/index.d.ts"
    },
    "./integrations": {
      "import": "./packages/integrations/dist/index.js",
      "require": "./packages/integrations/dist/index.js",
      "types": "./packages/integrations/dist/index.d.ts"
    },
    "./feature-flags": {
      "import": "./packages/feature-flags/dist/index.js",
      "require": "./packages/feature-flags/dist/index.js",
      "types": "./packages/feature-flags/dist/index.d.ts"
    },
    "./governance": {
      "import": "./packages/governance/dist/index.js",
      "require": "./packages/governance/dist/index.js",
      "types": "./packages/governance/dist/index.d.ts"
    },
    "./dev-server": {
      "import": "./packages/dev-server/dist/index.js",
      "require": "./packages/dev-server/dist/index.js",
      "types": "./packages/dev-server/dist/index.d.ts"
    },
    "./tooling": {
      "import": "./packages/tooling/dist/index.js",
      "require": "./packages/tooling/dist/index.js",
      "types": "./packages/tooling/dist/index.d.ts"
    },
    "./contract-testing": {
      "import": "./packages/contract-testing/dist/index.js",
      "require": "./packages/contract-testing/dist/index.js",
      "types": "./packages/contract-testing/dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "lint": "prettier --write \"{examples,packages}/**/*.ts\" && tsc -b --pretty false",
    "test": "npm run build && node --test \"tests/*.test.js\"",
    "dev": "ocd-dev --entry dist/examples/server/src/main.js --watch examples/server/src",
    "docs": "ocd-docs --module examples/server/dist/user/user.module.js",
    "prepack": "npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "typescript",
    "backend",
    "framework",
    "decorators",
    "dependency-injection",
    "rest-api",
    "express",
    "security",
    "observability",
    "orm",
    "cli"
  ],
  "author": "OCD-JS Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hidesec/ocd-js.git"
  },
  "bugs": {
    "url": "https://github.com/hidesec/ocd-js/issues"
  },
  "homepage": "https://github.com/hidesec/ocd-js#readme",
  "workspaces": {
    "packages": [
      "packages/*",
      "examples/*"
    ]
  },
  "dependencies": {
    "@ocd-js/cli": "file:./packages/cli",
    "@ocd-js/core": "file:./packages/core",
    "@ocd-js/auth": "file:./packages/auth",
    "@ocd-js/security": "file:./packages/security",
    "@ocd-js/observability": "file:./packages/observability",
    "@ocd-js/performance": "file:./packages/performance",
    "@ocd-js/plugins": "file:./packages/plugins",
    "@ocd-js/integrations": "file:./packages/integrations",
    "@ocd-js/governance": "file:./packages/governance",
    "@ocd-js/feature-flags": "file:./packages/feature-flags",
    "@ocd-js/orm": "file:./packages/orm",
    "@ocd-js/server": "file:./packages/server",
    "@ocd-js/testing": "file:./packages/testing",
    "@ocd-js/dev-server": "file:./packages/dev-server",
    "@ocd-js/tooling": "file:./packages/tooling",
    "@ocd-js/contract-testing": "file:./packages/contract-testing"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/sql.js": "^1.4.9",
    "prettier": "^3.3.2",
    "supertest": "^6.3.4",
    "typescript": "^5.4.5"
  }
}
